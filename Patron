import java.awt.*;
import java.awt.image.*;
import javax.imageio.ImageIO;
import javax.swing.*;
import java.io.*;
import java.util.*;

public class Patron
{
  private int value;
  private BufferedImage face;
  private TreeMap<String, Integer> cost;

  public Patron(int v, BufferedImage f, TreeMap<String, Integer> c)
  {
    face = f;
    cost = c;
    value = v;
  }
  
  public int getValue()
  {
	  return value;
  }

  public TreeMap<String, Integer> getCost()
  {
    return cost;
  }
}
